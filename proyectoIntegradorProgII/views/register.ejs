<%- include("./partials/head") %>
<%- include("./partials/header")%>

<!-- Novedades -->
<div class="container products-wrapper">
    <div class="row">
        <div class="col-12">
            <h2 class="products-title">Registrate</h2>
        </div>
        <!-- Show the "ultimos" products -->
        <h2 class="col-12 form_container">
            <form action="/users/login" method="get">
                <label for="email">Email: 
                    <input type="email" id="email" name="email" required>
                </label>
                <label for="usuario">Usuario:
                    <input type="text" id="usuario" name="usuario" required >
                </label>
                <label for="contrasenia">Contraseña:
                    <input type="password" id="contrasenia" name="contrasenia" required >
                </label>
                <label for="cumpleanios">Fecha de Nacimiento:
                    <input type="date" id="cumpleanios" name="cumpleanios" required>
                </label>
                <label for="dni">Número de documento:
                    <input type="number" id="dni" name="dni" required>
                </label>
                <label for="foto">Foto de perfil :
                    <input type="text" name="foto" id="foto">
                </label>
                <button type="submit">Registrarse</button>
            </form>
        </h2>
    </div>
</div>

<%- include("./partials/footer") %>